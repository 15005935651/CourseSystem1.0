<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zxx.entity.ManagerEntity" table="manager" schema="course_system">
        <id name="managerId" column="manager_id"/>
        <property name="managerPassword" column="manager_password"/>
        <property name="managerName" column="manager_name"/>
    </class>
</hibernate-mapping>