<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zxx.entity.StudentCourseEntity" table="student_course" schema="course_system">
        <id name="id" column="id"/>
        <property name="studentId" column="student_id"/>
        <property name="courseName" column="course_name"/>
        <property name="studentCourseScore" column="student_course_score"/>
        <property name="studentCourseCredit" column="student_course_credit"/>
        <property name="studentName" column="student_name"/>
        <property name="courseTime" column="course_time"/>
        <property name="courseTeacher" column="course_teacher"/>
    </class>
</hibernate-mapping>